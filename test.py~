import data
import binaries
import matplotlib.pyplot as plt

allkics = data.select_kics()
#print allkics

for n,kic in enumerate(allkics[:10]):
    binary=binaries.RealBinary(kic)
    #print binary.flux
    try:
        period,power = binary.periodogram()
	except ZeroDivisionError:
	    pass

